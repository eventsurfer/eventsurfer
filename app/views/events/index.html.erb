<h1>Willkommen zur Ãœbersicht alle Events</h1>

<div class="container-fluid">
  <div class="row">
    <div class="col-sm-3"></div>
    <div class="col-sm-6">
      <table class="table">
        <thead>
        <th>Id</th>
        <th>Name</th>
        <th>Freie Tickets</th>
        </thead>
        <tbody>
        <% @events.each do |event| %>
          <tr>
            <td><%= event.id %></td>
            <td><%= event.eventName %></td>
            <td><%= event.numberOfTickets %></td>
          </tr>
        <% end %>
        </tbody>
      </table>
    </div>
    <div class="col-sm-3"></div>
  </div>
</div>
